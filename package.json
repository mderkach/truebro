{
  "name": "truebro",
  "version": "1.0.0",
  "description": "truebro",
  "main": "./src/main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config ./build/webpack.dev.conf.js --mode development --progress",
    "server": "json-server --port 4000 --host 192.168.0.18 --watch ./src/db/db.json",
    "build": "npm run webp && cross-env NODE_ENV=production webpack --config ./build/webpack.prod.conf.js --mode production --progress",
    "lint": "eslint --debug src/",
    "lint:write": "eslint --debug src/ --fix",
    "stylelint": "stylelint ./src/**/*.scss",
    "stylelint:write": "stylelint ./src/**/*.scss --fix",
    "webp": "node ./build/webpack.webp.js"
  },
  "browserslist": [
    "> 0.25%, not dead",
    "last 3 version"
  ],
  "author": "https://github.com/mderkach",
  "keywords": [
    "webpack5",
    "scss",
    "es6",
    "react"
  ],
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "npm run lint:write",
      "git add"
    ],
    "*.(scss|sass|css)": [
      "npm run stylelint:write",
      "git add"
    ]
  },
